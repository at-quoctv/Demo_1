<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= form_tag search_path, :method => 'get', :id => "current" do %>

  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
  <%= will_paginate %>
  <ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
<% end %>



